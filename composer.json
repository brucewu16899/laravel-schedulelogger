{
  "name": "pendonl/laravel-schedulelogger",
  "description": "Log execution time of scheduled tasks in Laravel",
  "authors": [
    {
      "name": "Joshua de Gier",
      "email": "info@pendo.nl"
    }
  ],
  "require": {
    "php": ">=5.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "PendoNL\\LaravelScheduleLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PendoNL\\LaravelScheduleLogger\\Test\\": "tests"
    }
  },
  "license": "MIT",
  "extra": {
    "laravel": {
      "providers": [
        "PendoNL\\LaravelScheduleLogger\\LaravelScheduleLoggerServiceProvider"
      ],
      "aliases": {
        "LogSchedule": "PendoNL\\LaravelScheduleLogger\\Facade"
      }
    }
  }
}
